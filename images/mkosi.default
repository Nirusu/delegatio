[Distribution]
Distribution=arch

[Output]
Format=gpt_ext4
Bootable=yes
QCow2=yes
Output=image.qcow2
#KernelCommandLine=root=/dev/vda2 rw

[Packages]
Packages=
         openssh
         vim
         crictl
         critest
         kubeadm
         kubectl
         qemu-guest-agent
         socat
         kubelet
         ethtool
         containerd

[Validation]
Password=password

[Content]
Cache=.cache
Autologin=yes

[Host]
QemuHeadless=yes

[Partitions]
#RootSize=10G
#HomeSize=10G
